{
    "name": "Dlsite DarkMode",
    "version": "0.0.0",
    "manifest_version": 2,
    "description": "DLSiteの外観をDarkMode化",
    "icons": {
        "16": "",
        "32": "",
        "48": "",
        "128": "",
        "500": ""
    },
    "content_scripts": [
        {
            "matches": [
                "https://www.dlsite.com/*"
            ],
            "js": [
                "public/jquery-3.5.1.min.js",
                "src/js/index.js"
            ],
            "css": [
                "src/css/home.css"
            ]
        },
        {
            "matches": [
                "https://play.dlsite.com/*"
            ],
            "js": [
                "public/jquery-3.5.1.min.js",
                "src/js/index.js"
            ],
            "css": [
                "src/css/play.css"
            ]
        }
    ]
}