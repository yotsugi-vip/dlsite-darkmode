{
    "name": "Dlsite DarkMode",
    "version": "0.0.0",
    "manifest_version": 2,
    "description": "DLSiteの外観をDarkMode化",
    "icons": {

    },
    "content_scripts": [
        {
            "matches": [
                "https://www.dlsite.com/*"
            ],
            "js": [
                "public/jquery-3.5.1.min.js",
                "public/js/home.js"
                
            ],
            "css": [
                "public/style/home/home.css"
            ]
        },
        {
            "matches": [
                "https://play.dlsite.com/*"
            ],
            "js": [
                "public/jquery-3.5.1.min.js",
                "public/js/play.js"
            ],
            "css": [
                "public/style/play/play.css"
            ]
        }
    ]
}